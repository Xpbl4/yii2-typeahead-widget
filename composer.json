{
  "name": "xpbl4/yii2-typeahead-widget",
  "description": "Yii2 wrapper for the Twitter Typeahead widget",
  "type": "yii2-extension",
  "keywords": ["yii2","extension","widget","typeahead"],
  "homepage": "https://github.com/Xpbl4/yii2-typeahead-widget",
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Serge Mashkov",
      "email": "xpbl4@motorwars.ru",
      "homepage": "https://github.com/Xpbl4",
      "role": "Developer"
    }
  ],
  "support": {
    "issues": "https://github.com/Xpbl4/yii2-typeahead-widget/issues?state=open",
    "source": "https://github.com/Xpbl4/yii2-typeahead-widget"
  },
  "require": {
    "yiisoft/yii2": "~2.0.0",
    "yiisoft/yii2-bootstrap": "~2.0.0",
    "bower-asset/typeahead.js": "~0.11.0",
    "components/handlebars.js": "~3.0.0"
  },
  "require-dev": {
    "phpunit/phpunit": "4.*"
  },
  "autoload": {
    "psr-4": {
      "xpbl4\\typeahead\\": "src"
    }
  },
  "extra": {
    "asset-installer-paths": {
      "npm-asset-library": "vendor/npm",
      "bower-asset-library": "vendor/bower"
    },
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    },
    {
      "type": "git",
      "url": "https://github.com/Xpbl4/yii2-typeahead-widget.git"
    }
  ]
}
